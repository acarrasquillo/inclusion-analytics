<header class="navbar navbar-dark sticky-top bg-dark flex-md-nowrap p-0 shadow">
  <a class="navbar-brand col-md-3 col-lg-2 me-0 px-3" href="#">Analytics App</a>
  <button class="navbar-toggler position-absolute d-md-none collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#sidebarMenu" aria-controls="sidebarMenu" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
</header>

<div class="container-fluid">
  <div class="row">
    <nav id="sidebarMenu" class="col-md-3 col-lg-2 d-md-block bg-light sidebar collapse">
      <div class="position-sticky pt-3">
        <ul class="nav nav-tabs flex-column">
          {# dashboard #}
          <li class="nav-item dropdown">
            <a class="nav-link" href="../" role="button" aria-expanded="false">
              <span data-feather="bar-chart-2"></span>
              Dashboard
            </a>
          </li>
          <!-- views -->
          <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-expanded="false">
              <span data-feather="eye"></span>
              Programs Views
            </a>
            <ul class="dropdown-menu">
              <li><a class="dropdown-item" href="../views/add">
                <span data-feather="plus"></span>
                  Add
              </a></li>
            </ul>
          </li>

          <!-- views -->
          <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-expanded="false">
              <span data-feather="video"></span>
              Programs
            </a>
            <ul class="dropdown-menu">
              <li><a class="dropdown-item" href="../program/add">
                <span data-feather="plus"></span>
                  Add
              </a></li>
            </ul>
          </li>
          <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-expanded="false">
              <span data-feather="film"></span>
              Programs Genre
            </a>
            <ul class="dropdown-menu">
              <li>
                <a class="dropdown-item" href="../genre/add">
                  <span data-feather="plus"></span>
                  Add
                </a>
              </li>
            </ul>
          </li>
          <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-expanded="false">
              <span data-feather="tv"></span>
              Programs Networks
            </a>
            <ul class="dropdown-menu">
              <li>
                <a class="dropdown-item" href="../network/add">
                  <span data-feather="plus"></span>
                  Add
                </a>
              </li>
            </ul>
          </li>
          <!-- town -->
          <li class="nav-item dropdown">
            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-expanded="false">
              <span data-feather="home"></span>
              Viewers Hometowns
            </a>
            <ul class="dropdown-menu">
              <li>
                <a class="dropdown-item" href="../town/add">
                  <span data-feather="plus"></span>
                  Add
                </a>
              </li>
            </ul>
          </li>

        </ul>
      </div>
    </nav>

    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
      <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">

        <!-- display page header -->
        {% block main_header %}{% endblock %}

      </div>

      <!-- show error messages -->
      {% for message in get_flashed_messages() %}
      <div class="alert alert-warning flash" role="alert">
        {{ message }}
      </div>
      {% endfor %}

      <div>
        <!-- display main content -->
        {% block content %}{% endblock %}

      </div>

    </main>
  </div>
</div>
      
